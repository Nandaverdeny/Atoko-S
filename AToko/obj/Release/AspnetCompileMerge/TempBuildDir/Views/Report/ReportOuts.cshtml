<table class="table" id="reports">
    <thead>
        <tr>
            <th> ProductOutID </th>
            <th> Date </th>
            <th> Product Code </th>
            <th> Product Name </th>
            <th> Notes </th>
            <th> Qty </th>
            <th> Price </th>
            <th> SubTotal </th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in ViewBag.reportout)
        {
            <tr>
                <td>@item.ProductOutID</td>
                <td>@item.Date</td>
                <td>@item.ProductCode</td>
                <td>@item.ProductName</td>
                <td>@item.Notes</td>
                <td>@item.Qty</td>
                <td>@item.Price</td>
                <td>@item.Total</td>
            </tr>

        }
    </tbody>
</table>
<table>
    <tr></tr>
    <tr></tr>
    <tr>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
        <td>Total</td>
        <td><text id="total"></text></td>
    </tr>
</table>




